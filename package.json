{
  "name": "plantselector",
  "version": "1.0.0",
  "description": "Coursera Bootstrap course capstone project - plant selector website",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
